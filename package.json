{
  "scripts": {
    "dev": "concurrently \"yarn start\" \"yarn start:api\"",
    "dev:live": "NC_DEV_LIVE=true yarn start",
    "dev:prod": "concurrently \"yarn start\" \"yarn start:api:prod\"",
    "prod:live": "cross-env NODE_ENV=production WEBPACK_SERVE=true webpack serve --mode=production --config webpack/webpack.config.prod.babel.mjs",
    "start": "cross-env START_HOT=1 NODE_ENV=development webpack serve --mode=development --config webpack/webpack.config.dev.babel.mjs --static-directory ./static --static-public-path /static",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "test": "TZ=America/New_York NODE_ENV=test CONFIG_ENCRYPTION_KEY=cc0e93bb948088a5a35affafb6e4b47210a659f9e150f9a55a41222eaacf5520 TEST=all mocha --exit --exclude test/importer-espn.spec.mjs --exclude test/libs-server.get-players-table-view-results-debug.mjs --require test/global.mjs",
    "test:data-view-queries": "TZ=America/New_York NODE_ENV=test CONFIG_ENCRYPTION_KEY=cc0e93bb948088a5a35affafb6e4b47210a659f9e150f9a55a41222eaacf5520 TEST=all mocha --exit test/libs-server.data-view-queries.mjs --require test/global.mjs",
    "start:api": "NODE_ENV=development nodemon server.mjs",
    "start:api:prod": "NODE_ENV=production node server.mjs",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.prod.babel.mjs",
    "analyze": "cross-env OPEN_ANALYZER=true NODE_ENV=production webpack --config ./webpack/webpack.config.prod.babel.mjs",
    "deploy": "pm2 deploy server.pm2.config.js production --force && yarn load:main && yarn load:worker1 && yarn load:worker2",
    "deploy:dist": "rsync -av --exclude 'stats.html' dist/ league:/root/league/source/dist",
    "todo": "grep -rni todo app api common scripts",
    "count:lines": "git ls-files -- ':!:yarn.lock' ':!:./data' | xargs wc -l | tail -n 1",
    "load:main": "ssh -A league 'export NVM_DIR=\"$HOME/.nvm\" && [ -s \"$NVM_DIR/nvm.sh\" ] && . \"$NVM_DIR/nvm.sh\" && cd /root/league && git pull && git submodule update --init --recursive && yarn install'",
    "load:worker1": "ssh -A league-worker-1 'export NVM_DIR=\"$HOME/.nvm\" && [ -s \"$NVM_DIR/nvm.sh\" ] && . \"$NVM_DIR/nvm.sh\" && cd /root/league && git pull && git submodule update --init private && yarn install && pm2 reload import-live-odds-worker.pm2.config.js --env production && pm2 reload import-live-plays-worker.pm2.config.js --env production'",
    "load:worker2": "ssh -A league-worker-2 'export NVM_DIR=\"$HOME/.nvm\" && [ -s \"$NVM_DIR/nvm.sh\" ] && . \"$NVM_DIR/nvm.sh\" && cd /root/league && git pull && git submodule update --init private && yarn install'",
    "load:crontab:main": "ssh -A league 'export NVM_DIR=\"$HOME/.nvm\" && [ -s \"$NVM_DIR/nvm.sh\" ] && . \"$NVM_DIR/nvm.sh\" && cd /root/league && git pull && git submodule update --init --recursive && crontab /root/league/server/crontab-main.cron'",
    "load:crontab:worker1": "ssh -A league-worker-1 'export NVM_DIR=\"$HOME/.nvm\" && [ -s \"$NVM_DIR/nvm.sh\" ] && . \"$NVM_DIR/nvm.sh\" && cd /root/league && git pull && git submodule update --init private && crontab /root/league/server/crontab-worker-1.cron'",
    "load:crontab:worker2": "cp -f private/crontab/crontab-worker-2.cron $HOME/crontab/ && load_crontab_files",
    "load:logrotate:main": "ssh league 'cd /root/league && git pull && cp /root/league/server/logrotate-main /etc/logrotate.d/league-main'",
    "setup:node": "bash scripts/setup-node-version.sh",
    "export:schema": "ssh league 'pg_dump -U league_user -h localhost --dbname=league_production --schema-only --clean --no-owner --if-exists --file=/root/schema.postgres.sql --no-tablespaces' && scp league:/root/schema.postgres.sql ./db/ && awk 'NR==17{print \"SET search_path = public;\"} {print}' ./db/schema.postgres.sql > ./db/schema.tmp.sql && mv ./db/schema.tmp.sql ./db/schema.postgres.sql",
    "generate:formats": "node scripts/generate-named-formats.mjs",
    "analyze:format-coverage": "node scripts/analyze-format-coverage.mjs",
    "generate:format-data": "node scripts/generate-format-data.mjs",
    "debug:data-view": "NODE_ENV=production node scripts/debug-data-view.mjs"
  },
  "imports": {
    "#config": "./config/index.mjs",
