.stacked-metric
  display flex
  flex-direction column
  align-items center
  justify-content center
  padding 2px 4px
  min-width 60px
  flex 0 0 60px
  width 60px

.stacked-metric__value
  font-weight 500
  font-size 13px
  line-height 1.2

.stacked-metric__ranks
  font-size 10px
  color rgba(0, 0, 0, 0.55)
  line-height 1
  white-space nowrap
